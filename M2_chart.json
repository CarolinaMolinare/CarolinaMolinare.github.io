{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "description": "A line chart of M2 money supply in US 1960-2025",
  "data": {
    "url": "https://raw.githubusercontent.com/CarolinaMolinare/CarolinaMolinare.github.io/refs/heads/main/M2SL.csv",
    "format": {"type": "dsv", "delimiter": ";"}
  },
  "mark": "line",
  "title": {
    "text": "Money Supply",
    "subtitle": "M2 nominal money supply, US. Source FRED"
  },
  "encoding": {
    "x": {"field": "date", "type": "temporal"},
    "y": {"field": "monney_supply", "type": "quantitative"}
  }
}