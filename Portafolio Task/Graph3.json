{
  "config": {
    "font": "Circular Std",
    "text": {
      "color": "#676A86",
      "align": "left",
      "baseline": "middle",
      "dx": 7,
      "dy": 0,
      "fontSize": 11
    },
    "view": {
      "stroke": null,
      "continuousWidth": 400,
      "continuousHeight": 300,
      "discreteWidth": 400,
      "discreteHeight": 300
    },
    "bar": {
      "color": "#179fdb"
    },
    "line": {
      "color": "#e6224b"
    },
    "rule": {
      "color": "#676A86"
    },
    "area": {
      "opacity": 0.3
    },
    "point": {
      "filled": true,
      "size": 80,
      "color": "#e6224b",
      "opacity": 0.95
    },
    "geoshape": {
      "stroke": "white",
      "strokeWidth": 0.3
    },
    "rect": {
      "fill": "#d6d4d4",
      "opacity": 0.3
    },
    "axis": {
      "labelColor": "#676A86",
      "labelFontSize": 11,
      "labelFont": "Circular Std",
      "labelOpacity": 0.7,
      "tickColor": "#676A86",
      "tickOpacity": 0.5,
      "domainColor": "#676A86",
      "domainOpacity": 0.5,
      "gridColor": "#676A86",
      "gridDash": [
        2,
        2
      ],
      "gridOpacity": 0.5,
      "title": null,
      "titleColor": "#676A86",
      "titleOpacity": 0.8,
      "tickSize": 4
    },
    "axisXDiscrete": {
      "grid": false,
      "labelAngle": 0,
      "tickCount": 10,
      "tickOpacity": 0.5,
      "title": null
    },
    "axisYDiscrete": {
      "ticks": false,
      "labelPadding": 5
    },
    "axisXTemporal": {
      "grid": false,
      "ticks": true
    },
    "axisXQuantitative": {
      "grid": true
    },
    "axisYQuantitative": {
      "gridColor": "#676A86",
      "gridDash": [
        1,
        5
      ],
      "gridOpacity": 0.5,
      "ticks": false,
      "labelPadding": 5,
      "tickCount": 8,
      "titleAngle": 0,
      "titleAlign": "left",
      "titleBaseline": "bottom",
      "titleX": 0,
      "titleY": -5
    },
    "title": {
      "color": "#122b39E6",
      "subtitleColor": "#676A86",
      "font": "Circular Std",
      "subtitleFont": "Circular Std",
      "anchor": "start",
      "fontSize": 14,
      "subtitleFontSize": 12,
      "subtitlePadding": 4,
      "offset": 0,
      "frame": "group"
    },
    "legend": {
      "titleColor": "#122b39",
      "title": null,
      "labelColor": "#122b39",
      "labelFontSize": 11
    }
  },
  "layer": [
    {
      "mark": {
        "type": "point",
        "fillOpacity": 0.8
      },
      "encoding": {
        "detail": {
          "field": "LAD23NM",
          "type": "nominal"
        },
        "tooltip": [
          {
            "field": "LAD23NM",
            "title": "Local Authority"
          },
          {
            "field": "Childcare accessibility",
            "format": ".2",
            "title": "Places per 100 children"
          },
          {
            "field": "Gross disposable household income per head",
            "format": ",.0f",
            "title": "Income per head (\u00a3)"
          }
        ],
        "x": {
          "axis": {
            "gridOpacity": 0.2,
            "title": "Equivalent places per 100 children"
          },
          "field": "Childcare accessibility",
          "scale": {
            "domain": [
              10,
              46
            ]
          },
          "type": "quantitative"
        },
        "y": {
          "axis": {
            "gridOpacity": 0.2,
            "ticks": true,
            "title": "Gross disposable income per head (\u00a3), source: ONS",
            "labelExpr": "'\u00a3' + format(datum.value, ',')"
          },
          "field": "Gross disposable household income per head",
          "scale": {
            "domain": [
              13000,
              45000
            ],
            "clamp": true,
            "nice": false
          },
          "type": "quantitative"
        }
      },
      "title": {
        "text": "Income and Childcare Accessibility in England"
      },
      "transform": [
        {
          "filter": "(datum.LAD23NM !== 'City of London')"
        }
      ]
    },
    {
      "mark": {
        "type": "text",
        "align": {
          "expr": "datum.LAD23NM == 'Torridge' ? 'center' : datum.LAD23NM == 'Walsall' ? 'center' : 'left'"
        },
        "dx": {
          "expr": "datum.LAD23NM == 'Torridge' ? 0 : datum.LAD23NM == 'Walsall' ? 0 : 7"
        },
        "dy": {
          "expr": "datum.LAD23NM == 'Torridge' ? -10 : datum.LAD23NM == 'Walsall' ? 10 : 0"
        }
      },
      "encoding": {
        "detail": {
          "field": "LAD23NM",
          "type": "nominal"
        },
        "text": {
          "field": "LAD23NM",
          "type": "nominal"
        },
        "tooltip": [
          {
            "field": "LAD23NM",
            "title": "Local Authority"
          },
          {
            "field": "Childcare accessibility",
            "format": ".2",
            "title": "Places per 100 children"
          },
          {
            "field": "Gross disposable household income per head",
            "format": ",.0f",
            "title": "Income per head (\u00a3)"
          }
        ],
        "x": {
          "field": "Childcare accessibility",
          "type": "quantitative"
        },
        "y": {
          "field": "Gross disposable household income per head",
          "type": "quantitative"
        }
      },
      "transform": [
        {
          "filter": {
            "field": "LAD23NM",
            "oneOf": [
              "Torridge",
              "Walsall",
              "Cambridge",
              "St Albans",
              "Richmond upon Thames"
            ]
          }
        }
      ]
    }
  ],
  "data": {
    "url": "https://raw.githubusercontent.com/jhellingsdata/RADataHub/main/Chart%20Packs/Childcare/data/C5_income_childcare_accessibility.csv"
  },
  "height": 350,
  "width": 400,
  "$schema": "https://vega.github.io/schema/vega-lite/v5.17.0.json"
}